{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "baseUrl": "./src",
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "paths": {
      "*": ["./*"]
    },
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "ESNext"
  }
}
